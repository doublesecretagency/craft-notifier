{
  "private": true,
  "main": "webpack.mix.js",
  "devDependencies": {
    "cross-env": "^7.0",
    "laravel-mix": "^6.0",
    "pinia": "^2.0",
    "resolve-url-loader": "^4.0",
    "sass": "^1.43",
    "sass-loader": "^12.3",
    "vue": "^3.0",
    "vue-loader": "^17.0"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
